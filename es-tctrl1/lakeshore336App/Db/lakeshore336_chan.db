record(fanout, "$(Sys)$(Dev)FOut"){
  field(SELM, "All")
  field(LNK1, "$(Sys)$(Dev)T-I")
  field(LNK2, "$(Sys)$(Dev)T:C-I")
  field(LNK3, "$(Sys)$(Dev)Val:Sens-I")
  field(LNK4, "$(Sys)$(Dev)T-Sts")
  field(LNK5, "$(Sys)$(Dev)Alrm:High-Sts")
  field(LNK6, "$(Sys)$(Dev)Alrm:Low-Sts")
  field(SCAN, "Passive")
}

record(ai, "$(Sys)$(Dev)T-I"){
  field(DESC, "Temperature")
  field(EGU,  "K")
  field(PREC, "3")
  field(DTYP, "stream")
  field(SDIS, "$(Sys)$(Dev)T-Sts NPP MS")
  field(DISA, "16")
  field(INP,  "@lakeshore336.proto readFloat(KRDG?,$(CHAN)) $(PORT)")
}

record(ai, "$(Sys)$(Dev)T:C-I"){
  field(DESC, "Temperature")
  field(EGU,  "degC")
  field(PREC, "3")
  field(DTYP, "stream")
  field(SDIS, "$(Sys)$(Dev)Alrm-Calc_ NPP MS")
  field(DISA, "16")
  field(INP,  "@lakeshore336.proto readFloat(CRDG?,$(CHAN)) $(PORT)")
}

record(ai, "$(Sys)$(Dev)Val:Sens-I"){
  field(DESC, "Sensor Reading")
  field(DTYP, "stream")
  field(SDIS, "$(Sys)$(Dev)T-Sts NPP MS")
  field(DISA, "16")
  field(INP,  "@lakeshore336.proto readFloat(SRDG?,$(CHAN)) $(PORT)")
  field(PREC, "5")
}

record(calc, "$(Sys)$(Dev)Alrm-Calc_"){
  field(INPA, "$(Sys)$(Dev)Alrm:High-Sts NPP MS")
  field(INPB, "$(Sys)$(Dev)Alrm:Low-Sts NPP MS")
  field(INPC, "$(Sys)$(Dev)T-Sts NPP MS")
  field(CALC, "0")
}

record(bi, "$(Sys)$(Dev)Alrm:High-Sts"){
  field(DTYP, "stream")
  field(INP,  "@lakeshore336.proto readIntInt_1(ALARMST?,$(CHAN)) $(PORT)")
  field(ZNAM, "OK")
  field(ONAM, "ALARM")
  field(ZSV,  "NO_ALARM")
  field(OSV,  "MAJOR")
  field(FLNK, "$(Sys)$(Dev)Alrm-Calc_")
}

record(bi, "$(Sys)$(Dev)Alrm:Low-Sts"){
  field(DTYP, "stream")
  field(INP,  "@lakeshore336.proto readIntInt_2(ALARMST?,$(CHAN)) $(PORT)")
  field(ZNAM, "OK")
  field(ONAM, "ALARM")
  field(ZSV,  "NO_ALARM")
  field(OSV,  "MAJOR")
  field(FLNK, "$(Sys)$(Dev)Alrm-Calc_")
}

record(mbbi, "$(Sys)$(Dev)T-Sts"){
  field(DESC, "Sensor Status")
  field(DTYP, "stream")
  field(INP,  "@lakeshore336.proto readInt(RDGST?,$(CHAN)) $(PORT)")
  field(ZRVL, "0")
  field(ONVL, "1")
  field(TWVL, "16")
  field(THVL, "32")
  field(FRVL, "64")
  field(FVVL, "128")
  field(ZRST, "OK")
  field(ONST, "INVALID")
  field(TWST, "UNDERRANGE")
  field(THST, "OVERRANGE")
  field(FRST, "SENSOR ZERO")
  field(FVST, "SENSOR OVERRANGE")
  field(ZRSV, "NO_ALARM")
  field(ONSV, "MAJOR")
  field(TWSV, "MAJOR")
  field(THSV, "MAJOR")
  field(FRSV, "MAJOR")
  field(FVSV, "MAJOR")
  field(FLNK, "$(Sys)$(Dev)Alrm-Calc_")
}

record(ai, "$(Sys)$(Dev)T:Lim-RB"){
  field(DTYP, "stream")
  field(INP,  "@lakeshore336.proto readFloat(TLIMIT?,$(CHAN)) $(PORT)")
  field(SCAN, "$(SCAN)")
  field(EGU,  "K")
}

record(ao, "$(Sys)$(Dev)T:Lim-SP"){
  field(DTYP, "stream")
  field(OUT,  "@lakeshore336.proto writeFloat(TLIMIT,$(CHAN)) $(PORT)")
  field(FLNK, "$(Sys)$(Dev)T:Lim-RB")
  field(EGU,  "K")
  info(autosaveFields, "VAL")
}

record(calcout, "$(Sys)$(Dev)Cmd:Fltr-Calc_"){
  field(DTYP, "stream")
  field(OUT,  "@lakeshore336.proto writeIntFloatFloat(FILTER,$(CHAN)) $(PORT)")
  field(CALC, "0")
  field(FLNK, "$(Sys)$(Dev)Cmd:Fltr-Sts")
  field(INPA, "$(Sys)$(Dev)Cmd:Fltr-Sel")
  field(INPB, "$(Sys)$(Dev)Val:FltrP-SP")
  field(INPC, "$(Sys)$(Dev)Val:FltrW-SP")
}

record(bo, "$(Sys)$(Dev)Cmd:Fltr-Sel"){
  field(ZNAM, "OFF")
  field(ONAM, "ON")
  field(FLNK, "$(Sys)$(Dev)Cmd:Fltr-Calc_")
  info(autosaveFields, "VAL")
}

record(ao, "$(Sys)$(Dev)Val:FltrP-SP"){
  field(DRVL, "2")
  field(DRVH, "64")
  field(FLNK, "$(Sys)$(Dev)Cmd:Fltr-Calc_")
  info(autosaveFields, "VAL")
}

record(ao, "$(Sys)$(Dev)Val:FltrW-SP"){
  field(DRVL, "1")
  field(DRVH, "10")
  field(FLNK, "$(Sys)$(Dev)Cmd:Fltr-Calc_")
  info(autosaveFields, "VAL")
}

record(bi, "$(Sys)$(Dev)Cmd:Fltr-Sts"){
  field(DTYP, "stream")
  field(INP,  "@lakeshore336.proto readIntFloatFloat_1(FILTER?,$(CHAN)) $(PORT)")
  field(ZNAM, "OFF")
  field(ONAM, "ON")
  field(SCAN, "$(SCAN)")
}

record(ai, "$(Sys)$(Dev)Val:FltrP-RB"){
  field(DTYP, "stream")
  field(INP,  "@lakeshore336.proto readIntFloatFloat_2(FILTER?,$(CHAN)) $(PORT)")
  field(SCAN, "$(SCAN)")
}

record(ai, "$(Sys)$(Dev)Val:FltrW-RB"){
  field(DTYP, "stream")
  field(INP,  "@lakeshore336.proto readIntFloatFloat_3(FILTER?,$(CHAN)) $(PORT)")
  field(SCAN, "$(SCAN)")
}

record(stringout, "$(Sys)$(Dev)T:Name-SP"){
  field(DTYP, "stream")
  field(OUT,  "@lakeshore336.proto writeString(INNAME,$(CHAN)) $(PORT)")
  field(FLNK, "$(Sys)$(Dev)T:Name-RB")
  info(autosaveFields, "VAL")
}

record(stringin, "$(Sys)$(Dev)T:Name-RB"){
  field(DTYP, "stream")
  field(INP,  "@lakeshore336.proto readString(INNAME?,$(CHAN)) $(PORT)")
  field(SCAN, "$(SCAN)")
}
