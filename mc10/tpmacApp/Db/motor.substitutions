file "db/motor.db"
{

# ADDR ---> axis #
# NOTE ---> 2 asyn ports: 1) lowel comms ("P0"), and 2) Motor Record ("motor")
pattern
{ P,             M,               MOTOR,   PORT,    ADDR, DESC,     DTYP }
{ "XF:23ID2-OP", "{Slt:2-Ax:Y}Mtr", 	  "motor", "PO",    1,  "Slit 2 y-axis", asynMotor }
{ "XF:23ID2-OP", "{Slt:3-Ax:X}Mtr", 	  "motor", "PO",    2,  "Slit 3 x-axis", asynMotor }
{ "XF:23ID2-BI", "{Diag:2-Ax:Y}Mtr", 	  "motor", "PO",    3,  "Diag 2 y-axis", asynMotor }
}
